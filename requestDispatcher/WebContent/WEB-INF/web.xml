<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>requestDispatcher</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>fw1</servlet-name>
    <servlet-class>com.servlet.requestDispatcher.forwardServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fw1</servlet-name>
    <url-pattern>/fwServlet1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fw2</servlet-name>
    <servlet-class>com.servlet.requestDispatcher.forwardServlet2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fw2</servlet-name>
    <url-pattern>/fwServlet2</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ic1</servlet-name>
  	<servlet-class>com.servlet.requestDispatcher.includeServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ic1</servlet-name>
  	<url-pattern>/icServlet1</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>ic2</servlet-name>
  	<servlet-class>com.servlet.requestDispatcher.includeServlet2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ic2</servlet-name>
  	<url-pattern>/icServlet2</url-pattern>
  </servlet-mapping>
  
</web-app>